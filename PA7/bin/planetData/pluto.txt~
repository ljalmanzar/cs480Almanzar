# Geometry file (.obj)
model ../bin/planetData/planet.obj
# Center of rotation (Planet or xyz floats)
planet sun
# Direction of rotation (1 for clockwise -1 for counter)
dir 1
# Rotation speed (float)
rspeed 3
# Orbit Radius (float)
oradius 18
# Oribit speed (float)
ospeed 4
# Planet Radius
pradius 0.5
# Texture file
texture ../bin/planetData/pluto_map.jpg
end
