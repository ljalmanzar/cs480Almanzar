# Geometry file (.obj)
model ../../bin/planetData/planet.obj
# Center of rotation (Planet or xyz floats)
planet ../../bin/planetData/15thCentury/earth.txt
# Direction of rotation (1 for clockwise -1 for counter)
dir 1
# Rotation speed (float)
rspeed .00057
# Orbit Radius (float)
oradius 0.387
# Oribit speed (float)
ospeed 1.607
# Planet Radius 0.5
pradius 0.3
# Texture file
texture ../../bin/planetData/mercury_map.jpg
# Rings?
ring 0
# Tilt
tilt 0
end
